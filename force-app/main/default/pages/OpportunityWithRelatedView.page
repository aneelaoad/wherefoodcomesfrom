                                            </div>
                                            <div class="slds-col--padded slds-size--1-of-3 slds-m-bottom--small">
                                            </div>
                                            
                                            <apex:outputPanel rendered="{!selectedId==product.Id}" styleclass="slds-p-left_large slds-grid slds-wrap slds-grid--pull-padded slds-size--1-of-1 box">
                                                <div class="slds-col--padded slds-size--1-of-3 slds-m-bottom--small">
                                                    <!--Acreage-->
                                                    <label class="slds-form-element__label" for="input-id-01">Name</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputField value="{!product.Name}" rendered="{! !editMode}" />
                                                        <apex:inputField value="{!product.Name}" rendered="{! editMode}" />
                                                    </div>
                                                </div>
                                                <div class="slds-col--padded slds-size--1-of-3 slds-m-bottom--small">
                                                    <!--Acreage-->
                                                    <label class="slds-form-element__label" for="input-id-01">Trade Name</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputField value="{!product.Trade_Name__c}" rendered="{! !editMode}" />
                                                        <apex:inputField value="{!product.Trade_Name__c}" rendered="{! editMode}" />
                                                    </div>
                                                </div>
                                                <div class="slds-col--padded slds-size--1-of-3 slds-m-bottom--small">
                                                    <!--Address/Legal Description-->
                                                    <label class="slds-form-element__label" for="input-id-01">Activated On</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputField value="{!product.Activated_On__c}" rendered="{! !editMode}" />
                                                        <apex:inputField value="{!product.Activated_On__c}" rendered="{! editMode}" />
                                                    </div>
                                                </div>
                                                <div class="slds-col--padded slds-size--1-of-3 slds-m-bottom--small">
                                                    <!--Comments-->
                                                    <label class="slds-form-element__label" for="input-id-01">Deactivated On</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputField value="{!product.Deactivated_On__c}" rendered="{! !editMode}" />
                                                        <apex:inputField value="{!product.Deactivated_On__c}" rendered="{! editMode}" />
                                                    </div>
                                                </div>
                                                <div class="slds-col--padded slds-size--1-of-3">
                                                    <!--Crops-->
                                                    <label class="slds-form-element__label" for="input-id-01">Private</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputField value="{!product.Private__c}" rendered="{! !editMode}" />
                                                        <apex:inputField value="{!product.Private__c}" rendered="{! editMode}" />
                                                    </div>
                                                </div>
                                                <div class="slds-col--padded slds-size--1-of-3">
                                                    <!--Crops-->
                                                    <label class="slds-form-element__label" for="input-id-01">Private Label</label>
                                                    <div class="slds-form-element__control">
                                                        <apex:outputField value="{!product.Private_Label__c}" rendered="{! !editMode}" />
                                                        <apex:inputField value="{!product.Private_Label__c}" rendered="{! editMode}" />
                                                    </div>
                                                </div>
                                            </apex:outputPanel>
                                        </apex:repeat>
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </fieldset>
                        
                    </div>
                </apex:form>
            </apex:outputpanel>
            
            
        </div>
    </body>
